<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.completeadapterview">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NavigationDrawerMain"></activity>
        <activity android:name=".BottomNaV" />
        <activity android:name=".ToolBarStudy" />
        <activity android:name=".NavBarStudy" />
        <activity android:name=".TabWithIcons" />
        <activity android:name=".TabNormal" />
        <activity android:name=".TabStudyTypes" />
        <activity android:name=".FragmentStudyMain" />
        <activity android:name=".Tablayout" />
        <activity android:name=".RecyclerViewVertical" />
        <activity android:name=".RecyclerViewGrid" />
        <activity android:name=".RecyclerViewHorizontal" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>